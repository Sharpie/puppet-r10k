---
:cachedir: <%= @cachedir %>
<% unless @postrun.empty? -%>

:postrun:
<% @postrun.each do |arg| -%>
  - <%= arg %>
<% end -%>
<% end -%>
<% unless @sources.empty? -%>

:sources:
<% @sources.sort.each do |name, params| -%>
  <%= name %>:
<% params.sort.each do |key, value| -%>
    <%= key %>: <%= value %>
<% end -%>
<% end -%>
<% end -%>
